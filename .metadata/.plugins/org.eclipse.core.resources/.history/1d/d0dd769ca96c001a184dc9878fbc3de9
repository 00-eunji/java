package ex01;

public class Sample {

	public static void main(String[] args) {
		System.out.print("오늘은 3월 23일입니다.");
		System.out.println("점심 메뉴는 짜장면입니다.");
		
		//정수형변수 int [eng_name]
		int num1=100;
		int num2=200;
		System.out.println("num1=" + num1);
		System.out.println("num2=" + num2);
	}

}
