package ex02;

public class Sample {

	public static void main(String[] args) {
		//실수형 변수 float [eng_name]
		float num1 = 10.5f;
		float num2 = 20.3f;
		float num3 = num1 + num2f;
		float num4 = num2 - num1f;
		float num5 = num1 * num2f;
		float num6 = num2 / num1f;
		System.out.println("num1 + num2=" + num3);
		System.out.println("num2 - num1=" + num4);
		System.out.println("num1 * num2=" + num5);
		System.out.println("num2 / num1=" + num6);

	}

}
