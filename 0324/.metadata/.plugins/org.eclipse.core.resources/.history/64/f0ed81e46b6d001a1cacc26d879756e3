package ex01;

public class Sample {

	public static void main(String[] args) {
		
		// 중첩 if문
		int score = 88;
		String grade="";
		
		
		if (score>=90) {
			if(score>=95) {
				grade="A+";
			}else {
				grade="A0";
			}
		}else if(score>=80) {
			if(score>=85) {
				grade="B+";
			}
			else {
				grade="B0";
			} 
		}else if(score>=70) {
				if(score>=75) {
					grade="C+";
				}
				else {
					grade="C0";
				}
			}
		System.out.println("등급은 " + grade + "입니다.");
		
	
	
	
	
	}
}

		
		
		
		
			
	// main

//sample class
