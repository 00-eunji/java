package ex05;

public class Sample {

	public static void main(String[] args) {
		switch1();
		
		
		
		
		
		
	}// main method

	
	public static void switch1() {
		System.out.println("switch 예제");
		int time=11;
		switch(time) {
		case 9:
			System.out.println("회의를 합니다.");
			break;
		case 8:
			System.out.println("출근을 합니다.");
			break;
		case 11:
			System.out.println("업무를 봅니다.");
			break;
		default:
			System.out.println("외근을 합니다.");
		}
		
		
		
	} //method2
	
}//sample
